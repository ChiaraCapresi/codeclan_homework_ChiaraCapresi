answer <- function(x){
if(is.character(x)){
paste("The answer is:", x)
}else{
if(is.numeric(x)){
if(x < 10){
return(x)
}else{
return(x/10)
}
}
return("Invalid input")
}
}
answer("hello")
answer(5)
answer(34)
answer(TRUE)
